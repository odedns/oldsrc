<?xml version="1.0" encoding="UTF-8"?>
<dsefmts.xml>
     <!--DWGroupDef:DSE Sample application.Server.Formats-->
     <!-- SERVER FORMAT DEFs.    ================================================ -->
     <!--=========================================================================-->
     <!-- StarUp Operation Formats                                                -->
     <!--=========================================================================-->
     <fmtDef id="startupReqFmt">
          <record>
               <fString dataName="TID"/>
               <delim delimChar="#"/>
               <fString dataName="WKSContext"/>
               <delim delimChar="#"/>
               <fString dataName="WKSParentContext"/>
               <delim delimChar="#"/>
          </record>
     </fmtDef>


	 <!-- my cs formats -->
	 <fmtDef id="myReqFmt">
          <record>
               <fString dataName="BranchId"/>
          </record>
     </fmtDef>
	 <fmtDef id="myReplyFmt">
          <record>
               <fString dataName="BranchId"/>
          </record>
     </fmtDef>
     
      <fmtDef id="genReqFmt">             
          <record>
               <fString dataName="opName"/>
                <delim delimChar="#"/>
               <fString dataName="ctxName"/>
                <delim delimChar="#"/>
               <fString dataName="ctxData"/>              
               <compress/>
               <delim delimChar="#"/>              	              
          </record>
         
     </fmtDef>
     
	 <fmtDef id="genReplyFmt">
          <record>
           <fString dataName="opName"/>
            <delim delimChar="#"/>
            <fString dataName="ctxName"/>
             <delim delimChar="#"/>
               <fString dataName="ctxData"/>
               <compress/>
                <delim delimChar="#"/>
          </record>
     </fmtDef>
     
	<fmtDef id="applicationTablesRequestFormat">
		  <fdXML kCollInsertedOnFormat="no" createKCollOnUnformat="no"/>
	</fmtDef>
	<fmtDef id="applicationTablesReplyFormat">
		  <fdXML kCollInsertedOnFormat="no" createKCollOnUnformat="no"/>
	</fmtDef>

     <!--======================================================================================-->
     <!-- Journal Step Operation Formats                                                       -->
     <!--======================================================================================-->
     <fmtDef id="preSendJournalFmt">
          <hashtable>
               <fObject dataName="UserId"/>
               <fObject dataName="TID"/>
               <fObject dataName="HostBuff"/>
          </hashtable>
     </fmtDef>
     <fmtDef id="afterRecJournalFmt">
          <hashtable>
               <fObject dataName="TrxReplyCode"/>
               <fObject dataName="TrxErrorMessage"/>
          </hashtable>
     </fmtDef>
     <!--======================================================================================-->
     <!-- ==== BEGIN definition for CashOperations AC Sample Application ==== -->
     <!--======================================================================================-->
     <fmtDef id="ACCashOperationsDepositInputMapFmt">
          <record>
               <constant value="&lt;Deposit&gt;"/>
               <constant value="&lt;DepositRequest&gt;"/>
               <fXML unNamed="false">
                    <fString dataName="AccountNumber"/>
                    <fDate dataName="Date"/>
                    <fNumString dataName="Amount" showThousandsSep="no"/>
                    <fString dataName="BranchId"/>
               </fXML>
               <constant value="&lt;/DepositRequest&gt;"/>
               <constant value="&lt;/Deposit&gt;"/>
          </record>
     </fmtDef>
     <fmtDef id="ACCashOperationsDepositOutputMapFmt">
          <record>
               <constant value="&lt;Deposit&gt;"/>
               <constant value="&lt;DepositResult&gt;"/>
               <fXML unNamed="false">
                    <fString dataName="TrxReplyCode"/>
                    <fNumString dataName="AccountBalance" showThousandsSep="no"/>
                    <fString dataName="TrxErrorMessage"/>
               </fXML>
               <constant value="&lt;/DepositResult&gt;"/>
               <constant value="&lt;/Deposit&gt;"/>
          </record>
     </fmtDef>
     <fmtDef id="ACCashOperationsWithdrawalInputMapFmt">
          <record>
               <constant value="&lt;Withdrawal&gt;"/>
               <constant value="&lt;WithdrawalRequest&gt;"/>
               <fXML unNamed="false">
                    <fString dataName="AccountNumber"/>
                    <fDate dataName="Date"/>
                    <fNumString dataName="Amount" showThousandsSep="no"/>
                    <fString dataName="BranchId"/>
               </fXML>
               <constant value="&lt;/WithdrawalRequest&gt;"/>
               <constant value="&lt;/Withdrawal&gt;"/>
          </record>
     </fmtDef>
     <fmtDef id="eventPair">
          <record dataName="eventPair">
               <!--Format for a String-->
               <fString dataName="eventName"/>
               <delim delimChar="+"/>
               <fString dataName="notifierName"/>
               <delim delimChar="+"/>
          </record>
     </fmtDef>
     <fmtDef id="ACCashOperationsWithdrawalOutputMapFmt">
          <record>
               <constant value="&lt;Withdrawal&gt;"/>
               <constant value="&lt;WithdrawalResult&gt;"/>
               <fXML unNamed="false">
                    <fString dataName="TrxReplyCode"/>
                    <fNumString dataName="AccountBalance" showThousandsSep="no"/>
                    <fString dataName="TrxErrorMessage"/>
               </fXML>
               <constant value="&lt;/WithdrawalResult&gt;"/>
               <constant value="&lt;/Withdrawal&gt;"/>
          </record>
     </fmtDef>
     
     <fmtDef id="EventManagerCSFormat">
     	<record>
     		<fString dataName="operationType" /> <delim delimChar="+" />
     		<iCollF dataName="eventList" >
     			<refFmt refId="eventPair"/>
     		</iCollF>
     	</record>
     </fmtDef>
     <fmtDef id="eventPair" >
     	<record dataName="eventPair" >
     		<fString dataName="eventName" /> <delim delimChar="+" />
     		<fString dataName="notifierName" /> <delim delimChar="+" />
     	</record>
     </fmtDef>
     
     <fmtDef id="storeFormatName">
	    <hashtable>
    	    <fObject dataName="account_number" />
        	<fObject dataName="amount" /> 
	        <fObject dataName="date" />
    	    <fObject dataName="DESCRIPTION" /> 
	    </hashtable>
	</fmtDef>
	
	
	<fmtDef id="journalFormatName">
    	<hashtable>
        	<fObject dataName="accountNumber.branchNumber"/>
	        <fObject dataName="accountNumber.agreementNumber"/>
    	    <fObject dataName="dueDate"/> 
	    </hashtable> 
	</fmtDef>
	
<!-- reftables format -->
	 <fmtDef id="refTablesReqFmt">	 
	  <fdXML kCollInsertedOnFormat="no" createKCollOnUnformat="no"/>
     </fmtDef>
	

	<fmtDef id="refTablesReplyFmt">
	  <fdXML kCollInsertedOnFormat="no" createKCollOnUnformat="no"/>
	</fmtDef>


<!-- test the binary format -->	
     <fmtDef id="testBinary">
     	<fBinary dataName="refTablesRecord" />
     </fmtDef>

	<fmtDef id="matafOverrideTableFmt">
	    <hashtable>    	 
        	<fObject dataName="IDNUMBER"/>
        	<fObject dataName="TRXID"/>
	        <fObject dataName="STATUS"/> 
    	    <fObject dataName="REASON"/>
   	        <fObject dataName="OR_DATE"/>
    	</hashtable> 

     </fmtDef>

     <!--/DWGroupDef-->

</dsefmts.xml>
