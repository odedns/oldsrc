<?xml version="1.0" encoding="UTF-8"?>
<dseproc.xml>
     <!--DWGroupDef:DSE Sample application.Server.Processor-->
     
     	 <processor id="myproc" context="myCtx" implClass="composer.MyProcessor" >
			 <state id="initial" type="initial">
				<entryActions>
						<start id="startAct"/>
				</entryActions>
               <transitions>
					   <transition id="startAct.start" targetState="op1" />
			   </transitions>
			  </state>
			 <state id="op1" type="operation">
				<entryActions>
					<executeOperationAct id="op1" operationName="op1" 
							linkContextTo="processor" >
					</executeOperationAct>
				</entryActions>
               <transitions>
					   <transition id="op1.ok" targetState="op2" />
					   <transition id="op1.error" targetState="op3" />
					   <transition id="op1.foo" targetState="foo" />
			   </transitions>
			  </state>
			 <state id="op2" type="operation">
				<entryActions>
					<executeOperationAct id="op2" operationName="op2" 
							linkContextTo="processor" >
					</executeOperationAct>
				</entryActions>
               <transitions>
					   <transition id="op2.ok" targetState="op3" />
			   </transitions>
			  </state>

			 <state id="foo" type="operation">
				<entryActions>
					<executeOperationAct id="fooOp" operationName="fooOp" 
							linkContextTo="processor" >
					</executeOperationAct>
				</entryActions>
               <transitions>
					   <transition id="fooOp.ok" targetState="finalOk" />
			   </transitions>
			  </state>
			 <state id="op3" type="operation">
				<entryActions>
					<executeOperationAct id="op3" operationName="op3" 
							linkContextTo="processor" >
					</executeOperationAct>
				</entryActions>
               <transitions>
					   <transition id="op3.ok" targetState="finalOk" />
			   </transitions>
			  </state>
			  <state id="finalOk" type="final">
			  </state>
	</processor>
     <!--/DWGroupDef-->
</dseproc.xml>
