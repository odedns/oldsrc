<?xml version="1.0"?>
<!-- STD less than or equal to 2, with bins and batches/-->
<!-- VLT less than or equal to 2, no bins and batches/-->
<branch>
 <branchId>0000000001</branchId>
 <!-- Cash drawer definition/-->
 <cashDrawer currency="USD" type="STD" scale="2">
  <baitbills denomination="1" no="5" />
  <trays>WRK RS1 RS2</trays>
  <totals>CASH_IN CASH_OUT CHECK_BATCH HOLDOVER_CHECK_BATCH TRANSIT ON_US_CHECK WITHDRAWAL DEPOSIT TX_DEBIT TX_CREDIT</totals>
<!-- bins can be empty/-->
  <bins>TRANSIT ON_US</bins>
  <closeLimit>9000</closeLimit>
  <overShortLimit>9</overShortLimit>
  <drawerHighWorkingLimit>1000000</drawerHighWorkingLimit>
  <drawerLowWorkingLimit>100000</drawerLowWorkingLimit>
  <!-- Tray definition/-->
  <tray type="WRK">
<!-- trayHighLimit can be empty /-->
   <trayHighLimit>1100000</trayHighLimit>
<!-- trayLowLimit can be empty /-->
   <trayLowLimit>11000</trayLowLimit>
  </tray>
  <tray type="RS1">
   <trayHighLimit>2200000</trayHighLimit>
   <trayLowLimit>220000</trayLowLimit>
  </tray>
  <tray type="RS2">
   <trayHighLimit>3300000</trayHighLimit>
   <trayLowLimit>330000</trayLowLimit>
  </tray>

  <!-- Bin definition /-->
<!-- bin can have various bin /-->
  <bin type="TRANSIT">
<!-- binCountLimit can be empty/-->
   <binCountLimit>40</binCountLimit>
<!-- binAmountLimit can be empty/-->
   <binAmountLimit>1000000</binAmountLimit>
  </bin>
  <bin type="ON_US">
   <binCountLimit>40</binCountLimit>
   <binAmountLimit>1000000</binAmountLimit>
  </bin>
 </cashDrawer>

