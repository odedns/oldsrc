  <!-- cash Drawer ID Range /-->
 <idRange>
<!-- std can be empty /-->
  <std min="0000000001" max="0000000300" />
<!-- vlt can be empty /-->
  <vlt min="0000000500" max="0000000550" />
 </idRange>

 <!-- Totaling Rule/-->
 <txnCode code="DEPOSIT">
  <totalingRule action="INCREMENT" affects="DEPOSIT" txnAmountName="DEPOSIT" />
  <totalingRule action="INCREMENT" affects="CASH_IN" txnAmountName="CASH_IN" />
  <totalingRule action="INCREMENT" affects="CASH_OUT" txnAmountName="CASH_OUT" />
  <totalingRule action="INCREMENT" affects="TRANSIT" txnAmountName="TRANSIT" />
  <totalingRule action="INCREMENT" affects="ON_US_CHECK" txnAmountName="ON_US_CHECK" />
  <totalingRule action="ADD_BATCH_ITEM" affects="TRANSIT" txnAmountName="TRANSIT" />
  <totalingRule action="ADD_BATCH_ITEM" affects="ON_US" txnAmountName="ON_US_CHECK" />
 </txnCode>
 <txnCode code="WITHDRAWAL">
  <totalingRule action="INCREMENT" affects="WITHDRAWAL" txnAmountName="WITHDRAWAL" />
  <totalingRule action="INCREMENT" affects="CASH_OUT" txnAmountName="CASH_OUT" />
 </txnCode>

 <!-- Teller Exchange Totalling Rules/-->

 <txnCode code="TX_IN">
  <totalingRule action="INCREMENT" affects="CASH_IN" txnAmountName="CASH_IN" />
  <totalingRule action="INCREMENT" affects="TX_CREDIT" txnAmountName="CASH_IN" />
 </txnCode>

 <txnCode code="TX_OUT">
  <totalingRule action="INCREMENT" affects="CASH_OUT" txnAmountName="CASH_OUT" />
  <totalingRule action="INCREMENT" affects="TX_DEBIT" txnAmountName="CASH_OUT" />
 </txnCode>

 <!-- Post Over/Short Totalling Rules/-->

 <txnCode code="POSTSHORT">
  <totalingRule action="INCREMENT" affects="CASH_IN" txnAmountName="CASH_IN" />
  <totalingRule action="INCREMENT" affects="TX_CREDIT" txnAmountName="CASH_IN" />
 </txnCode>

 <txnCode code="POSTOVER">
  <totalingRule action="INCREMENT" affects="CASH_OUT" txnAmountName="CASH_OUT" />
  <totalingRule action="INCREMENT" affects="TX_DEBIT" txnAmountName="CASH_OUT" />
 </txnCode>


</branch>

