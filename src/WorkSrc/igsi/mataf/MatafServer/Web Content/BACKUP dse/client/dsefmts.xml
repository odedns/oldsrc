<?xml version="1.0" encoding="UTF-8"?>
<dsefmts.xml>
	<!-- CLIENT FORMAT DEFs.    ================================================ -->
	<!--=========================================================================-->
	<!-- StarUp Operation Formats                                                -->
	<!--=========================================================================-->
	<fmtDef id="startupReqFmt">
		<record>
			<fString dataName="TID" />
			<delim delimChar="#" />
			<nullCheck />
			<fString dataName="WKSContext" />
			<delim delimChar="#" />
			<nullCheck />
			<fString dataName="WKSParentContext" />
			<delim delimChar="#" />
			<nullCheck />
		</record>
	</fmtDef>
	<fmtDef id="eventPair">
		<record dataName="eventPair">
			<fString dataName="eventName" />
			<delim delimChar="+" />
			<fString dataName="notifierName" />
			<delim delimChar="+" />
		</record>
	</fmtDef>

	<!--=========================================================================-->
	<!-- StartUp Operation Formats                                                -->
	<!--=========================================================================-->
	<fmtDef id="startupReplyFmt">
		<record>
			<fString dataName="TID" />
			<delim delimChar="#" />
			<nullCheck />
			<fString dataName="WKSContext" />
			<delim delimChar="#" />
			<nullCheck />
			<fString dataName="WKSParentContext" />
			<delim delimChar="#" />
			<nullCheck />
			<iCollF dataName="snifList" times="*">
				<record dataName="snifData">
					<!--Format for a String-->
					<fString dataName="snifCode" />
					
					<delim delimChar="#" />
					<nullCheck />
					<!--Format for a String-->
					<fString dataName="snifText" />
					
					<delim delimChar="#" />
					<nullCheck />
				</record>
			</iCollF>
		</record>
	</fmtDef>

