			<nullCheck />
			<fNumString dataName="bankKupa" />
			
			<delim delimChar="#" />
			<nullCheck />
			<fNumString dataName="snifCode" />
			
			<delim delimChar="#" />
			<nullCheck />
			<fNumString dataName="sugCheshbonHlCode" />
			
			<delim delimChar="#" />
			<nullCheck />
			<fNumString dataName="cheshbon" />
			
			<delim delimChar="#" />
			<nullCheck />
			<fNumString dataName="misparHl" />
			
			<delim delimChar="#" />
