			<delim delimChar="#" />
			<nullCheck />
			<fNumString dataName="misparHl" />
			
			<delim delimChar="#" />
			<nullCheck />
			
			<fDate dataName="taarichMatan" />
			
			<delim delimChar="#" />
			<nullCheck /-->
