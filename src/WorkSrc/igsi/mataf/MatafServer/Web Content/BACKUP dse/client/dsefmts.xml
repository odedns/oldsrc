	<!--=========================================================================-->
    <!-- initHL Operation Formats                                             -->
    <!--=========================================================================-->
	<fmtDef id="HLinitReplyFmt">
		<record>
			<iCollF dataName="sugCheshbonHlList" times="*">
				<record dataName="sugCheshbonHlData">
					<fString dataName="sugCheshbonHlCode" />
					<delim delimChar="@" />
					<nullCheck />
					<fString dataName="sugCheshbonHlText" />
					<delim delimChar="@" />
					<nullCheck />
				</record>
			</iCollF>

			<delim delimChar="#" />

			<iCollF dataName="marechetList" times="*">
				<record dataName="marechetData">
					<fString dataName="marechetCode" />
					<delim delimChar="@" />
					<nullCheck />
					<fString dataName="marechetText" />
					<delim delimChar="@" />
					<nullCheck />
				</record>
			</iCollF>

			<delim delimChar="#" />

			<fString dataName="misparHl" />
			<delim delimChar="#" />
			<nullCheck />
		</record>
	</fmtDef>

	<fmtDef id="HLinitReqFmt">
		<record>
			<fString dataName="TID" />
			<nullCheck />
		</record>
	</fmtDef>	
	
	<!--======================================================================================-->
	<!-- DO NOT MODIFY!! Event Manager Formats                                                -->
	<!--======================================================================================-->
	<fmtDef id="EventManagerCSFormat">
		<record>
			<fString dataName="operationType" />
			<delim delimChar="+" />
			<iCollF dataName="eventList">
				<refFmt refId="eventPair" />
			</iCollF>
		</record>
	</fmtDef>
</dsefmts.xml>