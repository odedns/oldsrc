     <!-- SERVER FORMAT DEFs.    ================================================ -->
     <!--=========================================================================-->
     <!-- StarUp Operation Formats                                                -->
     <!--=========================================================================-->
     <fmtDef id="startupReqFmt">
          <record>
               <fString dataName="TID"/>
               <delim delimChar="#"/> <nullCheck/>
               <fString dataName="WKSContext"/>
               <delim delimChar="#"/> <nullCheck/>
               <fString dataName="WKSParentContext"/>
               <delim delimChar="#"/> <nullCheck/>
          </record>
     </fmtDef>
     <!--======================================================================================-->
     <!-- Journal Step Operation Formats                                                       -->
     <!--======================================================================================-->
     <fmtDef id="preSendJournalFmt">
          <hashtable>
               <fObject dataName="UserId"/>
