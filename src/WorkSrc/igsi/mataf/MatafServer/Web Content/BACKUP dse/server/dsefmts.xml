                         <!--Format for a String-->
                         <fString dataName="snifText"/>
                         <!--Appends a delimiter character after a formatted string-->
                         <delim delimChar="#"/> <nullCheck/>
                    </record>
               </iCollF>               
          </record>
     </fmtDef>
     
     
