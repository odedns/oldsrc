					<menuItem label="Using TBG" handler="ExportActionHandler" state="enabled" action="EXPORT_USING_TBG"/>
					<menuItem label="DTD files" handler="ExportActionHandler" state="enabled" action="EXPORT_DTD_FILES"/>
					<menuSeparator/>
