<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root SYSTEM "flower.dtd">
<root>
	<formatterDefinition name="agreementIdFMT" type="simple">
		<format fromName="selectedAgreementID" toName="agreementId" />
		<format fromName="mode" toName="mode" />			
	</formatterDefinition>

	<formatterDefinition name="agreementNewModeFMT" type="simple">
		<format fromValue="new" toName="mode"/>
	</formatterDefinition>

	<formatterDefinition name="agreementCopyModeFMT" type="simple">
		<format fromValue="copy" toName="mode"/>
	</formatterDefinition>

	<formatterDefinition name="agreementUpdateModeFMT" type="simple">
		<format fromValue="update" toName="mode"/>
	</formatterDefinition>

	<formatterDefinition name="customerFormatter" type="simple">
		<format fromName="customerId" toName="id"/>
		<format fromName="customerName" toName="firstName"/>				
	</formatterDefinition>

	<formatterDefinition name="custIDFormmater" type="simple">
		<format fromName="customerId" toName="custID"/>
	</formatterDefinition>


	<formatterDefinition name="customerOutFormatter" type="simple">
		<format fromName="customerId" toName="customerId"/>
		<format fromName="customerName" toName="customerName"/>				
	</formatterDefinition>



</root>