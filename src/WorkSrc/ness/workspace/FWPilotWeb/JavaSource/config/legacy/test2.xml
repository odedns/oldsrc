<?xml version="1.0" encoding="utf-8" ?> 
<!DOCTYPE root SYSTEM "legacy.dtd">

<root>

	<Record id="customerRecord" className="com.ness.test.legacy.Customer1">
	        <RecordField columnId="ID" columnType="integer" attributeId="id" uniqueId="true"/>
	        <RecordField columnId="FIRST_NAME" columnType="string" attributeId="firstName" />
	        <RecordField columnId="LAST_NAME" columnType="string" attributeId="lastName" />
	        <RecordField columnId="SEX" columnType="integer" attributeId="sex"/>
	        <RecordField columnId="SMOKING" columnType="integer" attributeId="smoking" setter="setSmokingInd" />
	        <RecordField columnId="BIRTH_DATE_STR" columnType="string" attributeId="birthDateStr" />
	        <RecordField columnId="BIRTH_DATE_STR" columnType="string"  attributeType="date" attributeId="birthDate2" fromMask="yyyy-MM-dd" />
	        <RecordField columnId="BIRTH_DATE" columnType="date" attributeType="date"  attributeId="birthDate" />
	        <RecordField columnId="TELEPHONE" columnType="integer" attributeId="telephone" />
	</Record>      

	<Record id="customerProfessionRecord" className="com.ness.test.legacy.CustomerProfession1">
	        <RecordField columnId="CUSTOMER_ID" columnType="integer" attributeId="customerId"/>
	        <RecordField columnId="PROFESSION_ID" columnType="integer" attributeId="professionId" />
	</Record>      


	<Record id="customerTelephoneRecord" className="com.ness.test.legacy.CustomerTelephone1">
	        <RecordField columnId="ID" columnType="integer" attributeId="id" />
	        <RecordField columnId="ADD_ID" columnType="integer" attributeId="adressID" />
	        <RecordField columnId="DESCRIPTION" columnType="string" attributeId="description" />
	</Record>      


	<Record id="customerAddressRecord" className="com.ness.test.legacy.CustomerAddress1">
	        <RecordField columnId="ID" columnType="integer" attributeId="id" uniqueId="true"/>
	        <RecordField columnId="PARENT_ID" columnType="integer" attributeId="customerId"/>
	        <RecordField columnId="CITY" columnType="string" attributeId="city"/>
	        <RecordField columnId="STREET" columnType="string" attributeId="street"/>
	        <RecordField columnId="STREET_NUMBER" columnType="integer" attributeId="streetNumber"/>
	        <RecordField columnId="MAIN" columnType="integer" attributeId="main"/>
	        <RecordField columnId="TELEPHONE" columnType="integer" attributeId="telephone"/>
	        <RecordField columnId="FAX" columnType="integer" attributeId="fax"/>
	</Record>      
	

	<LegacyCommand commandName="testCMD3" commandType="SP" sqlStatement="call SP_LEGACY3(?,?)" >
	
	        <CallArgument name="VAR01" type="integer" inputType="in" inputContainerGetter="getArg1Value"/>
	        
	        <CallArgument name="VAR02" type="integer" inputType="out" resultAs="simple" simpleResultSetter="setOutArg1"/>
	        
	
	        <LegacyObjectGraph className="com.ness.test.legacy.LegacyObjectGraph1">
	
		        <NodeRecord id="customerRecord" resultSetNumber="1" adder="addCustomer">
		        	<NodeRecord id="customerProfessionRecord" resultSetNumber="2" adder="addProfession" parentIdAttribute="customerId"/>
		        	<NodeRecord id="customerAddressRecord" resultSetNumber="3" adder="addAddress" parentIdAttribute="customerId"> 
		    			<NodeRecord id="customerTelephoneRecord" resultSetNumber="4" adder="addTelephone" parentIdAttribute="adressID"/> 
		    		</NodeRecord>		        	
		        </NodeRecord>
		        
	         </LegacyObjectGraph>
	         
	</LegacyCommand>

	
</root>