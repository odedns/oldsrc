<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root SYSTEM "flower.dtd">
<root>
	<buttonSet name="checkButtons">
		<button name="btnSame" eventName="btnSame" group="left" openWindowType="normal"/>
		<button name="btnDialog" eventName="btnDialog" group="left" openWindowType="dialog" windowExtraParams="scroll:no;status:no;dialogHeight:250px;dialogWidth:350px;dialogLeft:550px;dialogTop:250px;"/>
		<button name="btnPopup" eventName="btnPopup" group="left" openWindowType="popup" windowExtraParams="scroll=no,status=no,height=250,width=350,left=550,top=250"/>
	</buttonSet>

	<buttonSet name="dialogButtons">
		<button name="search" eventName="search" group="left"/>
		<button name="close" eventName="close" group="left" openWindowType="closeDialog" />
	</buttonSet>


	<context name="checkContext">
		<field name="checkTableModel" type="tableModel" />
		<field name="custId" type="xiInteger"  />
	</context>

	<context name="dialogContext">
		<field name="custId" type="xiInteger"  />
		<field name="customerName" type="xiString"  />
	</context>
</root>