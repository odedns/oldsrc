<?xml version="1.0"?>
<ejb-jar
       xmlns="http://java.sun.com/xml/ns/javaee"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                           http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
       version="3.0">
   <enterprise-beans>
      <session>
         <ejb-name>MytestBean</ejb-name>
         <remote>onjlib.ejb.mytest.MytestRemote</remote>
         <ejb-class>onjlib.ejb.mytest.MytestBean</ejb-class>
         <session-type>Stateless</session-type>
         <transaction-type>Container</transaction-type>
       </session>
       <session>
         <ejb-name>InvokerBean</ejb-name>
         <remote>onjlib.ejb.invoker.InvokerRemote</remote>
         <ejb-class>onjlib.ejb.invoker.InvokerBean</ejb-class>
         <session-type>Stateless</session-type>
         <transaction-type>Container</transaction-type>
       </session>
       
         <session>
         <ejb-name>DispatcherBean</ejb-name>
         <remote>onjlib.ejb.dispatcher.DispatcherRemote</remote>
         <local>onjlib.ejb.dispatcher.DispatcherLocal</local>
         <ejb-class>onjlib.ejb.dispatcher.DispatcherBean</ejb-class>
         <session-type>Stateless</session-type>         
         <transaction-type>Container</transaction-type>
       </session>
   </enterprise-beans>
</ejb-jar>

