<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar id="ejb-jar_ID">
	<display-name>HoshenEJBUtil</display-name>
	<enterprise-beans>
		<session id="Invoker">
			<ejb-name>Invoker</ejb-name>
			<home>hoshen.ejb.dynproxy.InvokerHome</home>
			<remote>hoshen.ejb.dynproxy.Invoker</remote>
			<ejb-class>hoshen.ejb.dynproxy.InvokerBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
	</enterprise-beans>
	<ejb-client-jar>HoshenEJBUtilClient.jar</ejb-client-jar>
</ejb-jar>
