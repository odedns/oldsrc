<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar id="ejb-jar_ID" version="2.1" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
	<display-name>
	HoshenSched</display-name>
	<enterprise-beans>
		<session id="HoshenProcess">
			<ejb-name>HoshenProcess</ejb-name>
			<home>com.ibm.websphere.scheduler.TaskHandlerHome</home>
			<remote>com.ibm.websphere.scheduler.TaskHandler</remote>
			<ejb-class>hoshen.scheduler.ejb.HoshenProcessBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
		<session id="ReceiveNotification">
			<ejb-name>ReceiveNotification</ejb-name>
			<home>com.ibm.websphere.scheduler.NotificationSinkHome</home>
			<remote>com.ibm.websphere.scheduler.NotificationSink</remote>
			<ejb-class>hoshen.scheduler.ejb.ReceiveNotificationBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
		<session id="HoshenStartupBean">
			<ejb-name>HoshenStartupBean</ejb-name>
			<home>com.ibm.websphere.startupservice.AppStartUpHome</home>
			<remote>com.ibm.websphere.startupservice.AppStartUp</remote>
			<ejb-class>hoshen.startup.ejb.HoshenStartupBean</ejb-class>
			<session-type>Stateful</session-type>
			<transaction-type>Container</transaction-type>
		</session>
	</enterprise-beans>
	<assembly-descriptor>
		<container-transaction>
			<method>
				<ejb-name>ReceiveNotification</ejb-name>
				<method-name>*</method-name>
			</method>
			<trans-attribute>NotSupported</trans-attribute>
		</container-transaction>
		<container-transaction>
			<method>
				<ejb-name>HoshenProcess</ejb-name>
				<method-name>*</method-name>
			</method>
			<method>
				<ejb-name>HoshenProcess</ejb-name>
				<method-intf>Remote</method-intf>
				<method-name>*</method-name>
			</method>
			<method>
				<ejb-name>HoshenStartupBean</ejb-name>
				<method-name>*</method-name>
			</method>
			<trans-attribute>Required</trans-attribute>
		</container-transaction>
	</assembly-descriptor>
</ejb-jar>

