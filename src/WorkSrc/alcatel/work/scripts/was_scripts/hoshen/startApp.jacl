
# start an application
	if { $argc > 0 } {
		set appName       [lindex $argv 0]  
		set node [$AdminControl getNode]
		set appManager [$AdminControl queryNames type=ApplicationManager,node=$node,process=server1,*]
		puts "Trying to start application: $appName"
		$AdminControl invoke $appManager startApplication $appName
		puts "started application: $appName"
	} else { 
   		puts "Usage wsadmin.sh -f startApp.jacl <applicationName> "
	}

