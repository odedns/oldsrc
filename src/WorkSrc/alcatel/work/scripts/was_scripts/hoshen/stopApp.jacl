
# stop an application
	if { $argc > 0 } {
		set appName       [lindex $argv 0]  
		set node [$AdminControl getNode]
		set appManager [$AdminControl queryNames type=ApplicationManager,node=$node,process=server1,*]
		puts "Trying to stopp application: $appName"
		$AdminControl invoke $appManager stopApplication $appName
		puts "stopped application: $appName"
	} else { 
   		puts "Usage wsadmin.sh -f stopApp.jacl <applicationName> "
	}

